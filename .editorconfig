# Front End Kit Syntax Rule
root = true

[*]
indent_style = space
indent_size = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[package.json]
indent_size = 4

[bower.json]
indent_size = 4

[*.yml]
indent_size = 4

[*.md]
trim_trailing_whitespace = false
